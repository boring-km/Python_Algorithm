# 동전 0

first = input().split()
count = int(first[0])
price = int(first[1])

arr = []
for i in range(count):
    arr.append(int(input()))
mok = 0
for i in range(len(arr)-1, -1, -1):
    if arr[i] <= price:
        #print("mok: ", mok, " arr[i]: ", arr[i], " i: ", i)
        mok += int(price / arr[i])
        price = price % arr[i]

print(int(mok))
